if MsgBox(68, "This will open a notepad program and send it some text.\nContinue?", "Send Test") = "No" then
    Exit
End If

ProcStart("notepad.exe")

hwnd$ = 0

Do Until hwnd$ <> 0
	hwnd$ = WinGetHandle("Untitled - Notepad")
Loop

WinActivate(hwnd$)
Send("Hello{!} I am a notepad application{!}")
