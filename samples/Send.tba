MsgBox(68, "This will open a notepad program and send it some text.\nContinue?", "Send Test")
IF @return = "No" THEN
	exit
END IF

run("notepad.exe")

hwnd$ = 0

do until hwnd$ <> 0
	hwnd$ = WinGetHandle("Untitled - Notepad")
loop

WinActivate(hwnd$)
send("Hello{!} I am a notepad application{!}")